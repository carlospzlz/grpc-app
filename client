#!/usr/bin/env python3

import grpc

import app_pb2
import app_pb2_grpc

def main():
    with grpc.insecure_channel('localhost:50051') as channel:
        stub = app_pb2_grpc.DataServiceStub(channel)
        response = stub.GetData(app_pb2.DataRequest(name='hello'))
        print("Received: " + response.label)

if __name__ == '__main__':
    main()
